{
  "dirs": {
    "ROOT": "./output",
    "RAW_TRACKS": "raw_tracks",
    "METADATA": "metadata",
    "PROCESSED_SAMPLES": "samples",
    "FEATURES": "features",
    "TRAIN_TEST_SPLITS": "train_test_split"
  },
  "metadata": {
